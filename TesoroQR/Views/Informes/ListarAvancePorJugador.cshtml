@model IEnumerable<TesoroQR.Models.Tesoro.Avance>

@{
    ViewBag.Title = "ListarAvancePorJugador";
}

<h2>ListarAvancePorJugador</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UltimaPista)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UltimaPista)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AvanceID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AvanceID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AvanceID })
        </td>
    </tr>
}

</table>
