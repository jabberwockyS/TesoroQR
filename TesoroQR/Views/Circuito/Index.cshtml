@model IEnumerable<TesoroQR.Models.Tesoro.Circuito>

@{
    ViewBag.Title = "Index";
}

<h2>Lista de Caminos</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Fecha)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CircuitoID }) |
            @Html.ActionLink("Details", "Details", new { id=item.CircuitoID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CircuitoID })
        </td>
    </tr>
}

</table>
